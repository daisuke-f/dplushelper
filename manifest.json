{
  "manifest_version": 2,
  "name": "d+helper",
  "description": "Disney+ helper",
  "version": "0.1",
  "icons": {
    "48": "icons/favicon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://disneyplus.disney.co.jp/view/*"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "content_script.css"
      ]
    }
  ]
}
